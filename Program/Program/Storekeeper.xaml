<Window x:Class="Курсовой_проект.Storekeeper"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Курсовой_проект"
        mc:Ignorable="d"
        xmlns:command="clr-namespace:Курсовой_проект.Commands"
        Title="Storekeeper" Height="775" Width="1080" Icon="icon.png">
    <Window.CommandBindings>
    </Window.CommandBindings>
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Content="Здравствуйте" HorizontalAlignment="Left" Margin="0" VerticalAlignment="Center"/>
            <TextBlock Name="TX_Name" Text="{Binding Фамилия}" HorizontalAlignment="Left" Margin="45,0,0,0" TextWrapping="Wrap" VerticalAlignment="Center"/>
            <TextBlock Name="TX_Name1" Text="{Binding Имя}" HorizontalAlignment="Left" Margin="50,0,0,0" TextWrapping="Wrap" VerticalAlignment="Center" />
        </StackPanel>
        
        <Frame x:Name="StoreMain" Source="PriemkaPage.xaml" Height="710"/> 
    </StackPanel>
    
</Window>
